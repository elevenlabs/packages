  "funding": {
    "type": "individual",
    "url": "https://elevenlabs.io/funding"
  },
  "config": {
    "port": 3000,
    "apiVersion": "v1",
    "maxUploadSize": "100mb"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.test.[jt]s?(x)",
      "**/?(*.)+(spec|test).[jt]s?(x)"
    ],
    "coverageDirectory": "./coverage",
    "collectCoverageFrom": [
      "src/**/*.{js,ts}",
      "!src/**/*.d.ts",
      "!src/**/__tests__/**"
    ]
  },
  "volta": {
    "node": "18.17.0",
    "npm": "9.6.7"
  },
  "packageManager": "npm@9.6.7"
