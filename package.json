    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/elevenlabs/elevenlabs-io.git",
    "directory": "/"
  },
  "homepage": "https://elevenlabs.io",
  "bugs": {
    "url": "https://github.com/elevenlabs/elevenlabs-io/issues",
    "email": "support@elevenlabs.io"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "private": true,
  "publishConfig": {
    "access": "restricted",
