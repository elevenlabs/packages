  "files": [
    "dist/",
    "src/",
    "LICENSE",
    "OWNERSHIP_AGREEMENT.md",
    "README.md",
    "CHANGELOG.md"
  ],
  "dependencies": {
    "@elevenlabs/voice-sdk": "^2.0.0",
    "axios": "^1.6.0",
    "dotenv": "^16.3.0",
    "express": "^4.18.0",
    "openai": "^4.0.0",
    "ws": "^8.14.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "@types/express": "^4.17.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@types/ws": "^8.5.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "esbuild": "^0.19.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.0.0",
    "jest": "^29.6.0",
    "prettier": "^3.0.0",
    "serve": "^14.0.0",
    "snyk": "^1.1200.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
